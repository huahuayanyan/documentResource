{
  "zValidate": {
    "ruleName": ["require","english","number","telephone","ip","mobile","mail","identity"
    ,"chinese","url","numberValidate","billsNo"],
    "require":{
      "rule": "IS_NOT_NULL",
      "errMsg": "不能为空"
    },
    "english":{
      "rule": "/[a-zA-Z]/",
      "errMsg": "英文格式"
    },
    "number":{
      "rule": "/^[0-9]*$/",
      "errMsg": "请输入非负整数"
    },
    "numberValidate":{
      "rule": "/^(\\-?)([0-9]{1,11})+(.[0-9]{1,2})?$/",
      "errMsg": "11位整数可有2位小数金额格式"
    },
    "telephone":{
      "rule": "/^0(10|2[0-5789]|\\d{3})-\\d{7,8}$/",
      "errMsg": "固话格式不正确"
    },
    "ip":{
      "rule": "/^((\\d|[1-9]\\d|1\\d\\d|2[0-4]\\d|25[0-5]|[*])\\.){3}(\\d|[1-9]\\d|1\\d\\d|2[0-4]\\d|25[0-5]|[*])$/",
      "errMsg": "ip格式不正确"
    },
    "mobile":{
      "rule": "/^1(3[0-9]|5[0-35-9]|8[0235-9])\\d{8}$/",
      "errMsg": "手机号码格式不正确"
    },
    "email":{
      "rule": "/^([a-zA-Z0-9]+[_|\\_|\\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\\_|\\.]?)*[a-zA-Z0-9]+\\.[a-zA-Z]{2,3}$/",
      "errMsg": "邮箱格式不正确"
    },
    "identity":{
      "rule": "/((11|12|13|14|15|21|22|23|31|32|33|34|35|36|37|41|42|43|44|45|46|50|51|52|53|54|61|62|63|64|65|71|81|82|91)\\d{4})((((19|20)(([02468][048])|([13579][26]))0229))|((20[0-9][0-9])|(19[0-9][0-9]))((((0[1-9])|(1[0-2]))((0[1-9])|(1\\d)|(2[0-8])))|((((0[1,3-9])|(1[0-2]))(29|30))|(((0[13578])|(1[02]))31))))((\\d{3}(x|X))|(\\d{4}))/",
      "errMsg": "省份证格式不正确"
    },
    "chinese":{
      "rule": "/^([\u4E00-\uFA29]|[\uE7C7-\uE7F3])*$/",
      "errMsg": "输入中文"
    },
    "url":{
      "rule": "/^http[s]?://[\\w\\.\\-]+$/",
      "errMsg": "url格式不正确"
    },
    "billsNo":{
      "rule": "/^[a-zA-Z_0-9-]{1,50}$/",
      "errMsg": "50位以内大小写英文,数字,_ 或者-"
    }

  }
}